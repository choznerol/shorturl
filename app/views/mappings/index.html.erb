<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Target url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mappings.each do |mapping| %>
